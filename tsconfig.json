{
    "compilerOptions": {
        "sourceMap": true,
        "esModuleInterop": true,
        "module": "commonjs",
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node",
        "rootDir": "./",
        "outDir": "./dist",
        "target": "es2017"
    },
    "include": [
        "src/**/*",
        "./*"
    ]
}